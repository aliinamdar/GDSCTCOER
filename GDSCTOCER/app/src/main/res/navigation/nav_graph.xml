<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/infoFragment">

    <fragment
        android:id="@+id/infoFragment"
        android:name="com.example.gdsctocer.InfoFragment"
        android:label="fragment_info2"
        tools:layout="@layout/fragment_info2" >
        <action
            android:id="@+id/action_infoFragment_to_gdscFragment"
            app:destination="@id/gdscFragment" />
    </fragment>
    <fragment
        android:id="@+id/gdscFragment"
        android:name="com.example.gdsctocer.GdscFragment"
        android:label="fragment_gdsc2"
        tools:layout="@layout/fragment_gdsc2" >
        <action
            android:id="@+id/action_gdscFragment_to_communityFragment"
            app:destination="@id/communityFragment" />
    </fragment>
    <fragment
        android:id="@+id/communityFragment"
        android:name="com.example.gdsctocer.CommunityFragment"
        android:label="fragment_community"
        tools:layout="@layout/fragment_community" />
</navigation>